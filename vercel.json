{
  "version": 2,
  "builds": [
    {
      "src": "signaling-server.js",
      "use": "@vercel/node"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "*.{js,css}",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/ws",
      "dest": "signaling-server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
